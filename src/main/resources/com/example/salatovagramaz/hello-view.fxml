<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="390.0" prefWidth="615.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.salatovagramaz.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" alignment="CENTER" contentDisplay="CENTER" prefHeight="44.0" prefWidth="444.0" text="zadaj počet sačkov, kolko chceš použiť a klikni ake šaláty chceš spraviť.">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="numbOfSack" alignment="CENTER" prefHeight="35.0" prefWidth="582.0">
      <font>
         <Font size="14.0" />
      </font></TextField>
    <Button onAction="#onVelkySal" prefHeight="69.0" prefWidth="126.0" text="Velky šalát">
      <font>
         <Font size="14.0" />
      </font></Button>
    <Button onAction="#onMalySal" prefHeight="67.0" prefWidth="127.0" text="Záhradný šalát">
      <font>
         <Font size="14.0" />
      </font></Button>
</VBox>
